82 {\cf20 /* creates singleton instance. as this function is static included to different}\par
83 {\cf20  * translation units, multiple instances of the static variables are created.}\par
84 {\cf20  * to make sure it is really a singleton, these instances must know each other}\par
85 {\cf20  * somethow. an environment variable is used for that purpose, ie. the address}\par
86 {\cf20  * of the singleton allocated is stored by the operating system.}\par
87 {\cf20  * this implementation is not thread-safe. */}\par
88 {\cf17 static} DebugmallocData * debugmalloc_singleton({\cf18 void}) \{\par
89     {\cf17 static} {\cf18 char} envstr[100];\par
90     {\cf17 static} {\cf18 void} *instance = NULL;\par
91 \par
92     {\cf20 /* if we do not know the address of the singleton:}\par
93 {\cf20      * - maybe we are the one to create it (env variable also does not exist)}\par
94 {\cf20      * - or it is already created, and stored in the env variable. */}\par
95     {\cf19 if} (instance == NULL) \{\par
96         {\cf18 char} envvarname[100] = {\cf22 ""};\par
97         sprintf(envvarname, {\cf22 "%s%d"}, {\cf22 "debugmallocsingleton"}, ({\cf18 int}) getpid());\par
98         {\cf18 char} *envptr = getenv(envvarname);\par
99         {\cf19 if} (envptr == NULL) \{\par
100             {\cf20 /* no env variable: create singleton. */}\par
101             instance = debugmalloc_create();\par
102             sprintf(envstr, {\cf22 "%s=%p"}, envvarname, instance);\par
103             putenv(envstr);\par
104         \} {\cf19 else} \{\par
105             {\cf20 /* another copy of this function already created it. */}\par
106             {\cf18 int} ok = sscanf(envptr, {\cf22 "%p"}, &instance);\par
107             {\cf19 if} (ok != 1) \{\par
108                 fprintf(stderr, {\cf22 "debugmalloc: nem lehet ertelmezni: %s!\\n"}, envptr);\par
109                 abort();\par
110             \}\par
111         \}\par
112     \}\par
113 \par
114     {\cf19 return} (DebugmallocData *) instance;\par
115 \}\par
